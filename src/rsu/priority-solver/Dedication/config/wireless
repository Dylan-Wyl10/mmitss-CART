config wifi-device  wifi0
        option type     atheros
	    option channel '178'
        option chanbw	'0'
        option agmode	'11a'
        option diversity        '0'
        option disabled '0'

        # REMOVE THIS LINE TO ENABLE WIFI:

config wifi-iface
        option network  dsrcnet0	# applies to DSRC Radio 0-do not modify
        option device   wifi0		# applies to DSRC Radio 0-do not modify
        option hidden   '0'
        option isolate  '0'
# set the rate to 3M for more reliability and distance, 
# 3M for 10mhz channel and 6M for 20mhz channel
	    option rate	    'auto'
# don't set the tx power as driver chooses maximum supported by chip
#	    option txpower	'16'
        option bgscan   '0'
        option wds      '0'
        option macpolicy        'none'

        # AH Demo mode
        option mode     'ahdemo'	# radio mode ahdemo(DSRC),
                                        # ap(Access Point), sta(WiFi client)
                                        # All security parameters ignored in
                                        # ahdemo mode.
        option ssid     'gemsdsrcnet'	# SSID to advertize(AP mode) or
                                        # connect to (client mode)
        #option encryption       none	# security none/WEP/PSK(WPA)/PSK2(WPA2)
        #option key      1		# key number in case of WEP
        #option key      'somekey8'	# security key for WEP or WPA or WPA2.
                                        # Must be 10 or 26 hex digits for WEP.
                                        # Must be atleast 8 characaters for
	                                # WPA/WPA2.


        # Station mode - no ecnryption
        #option ssid     'gems'
        #option mode     'sta'
        #option encryption       none

        # Station mode - WAP
        #option ssid     'some-ssid'
        #option mode     'sta'
        #option encryption       'psk'
        #option key      'somekey'

        # Station mode - WEP
        #option mode     sta
        #option ssid     some-ssid
        #option encryption wep
        #option key      1
        #option key1     '36241D3FFEF916BE7DCF995FD9'

        # AP mode
        #option mode     ap
        #option encryption none
        #option ssid     OpenWrt

config wifi-device  wifi1
        option type     atheros
	    option channel '172'
        option chanbw	'0'
        option agmode	'11a'
        option diversity        '0'
        option disabled '0'

        # REMOVE THIS LINE TO ENABLE WIFI:

config wifi-iface
        option network  dsrcnet1	# applies to DSRC Radio 1-do not modify
        option device   wifi1		# applies to DSRC Radio 1-do not modify
        option hidden   '0'
        option isolate  '0'
# set the rate to 3M for more reliability and distance, 
# 3M for 10mhz channel and 6M for 20mhz channel
	    option rate	    'auto'
# don't set the tx power as driver chooses maximum supported by chip
#	    option txpower	'16'
        option bgscan   '0'
        option wds      '0'
        option macpolicy        'none'

        # AH Demo mode
        option mode     'ahdemo'	# radio mode ahdemo(DSRC),
                                        # ap(Access Point), sta(WiFi client)
                                        # All security parameters ignored in
                                        # ahdemo mode.
        option ssid     'gemsdsrcnet'	# SSID to advertize(AP mode) or
                                        # connect to (client mode)
        #option encryption       none	# security none/WEP/PSK(WPA)/PSK2(WPA2)
        #option key      1		# key number in case of WEP
        #option key      'somekey8'	# security key for WEP or WPA or WPA2.
                                        # Must be 10 or 26 hex digits for WEP.
                                        # Must be atleast 8 characaters for
	                                # WPA/WPA2.

        # Station mode - no ecnryption
        #option ssid     'gems'
        #option mode     'sta'
        #option encryption       none

        # Station mode - WAP
        #option ssid     'some-ssid'
        #option mode     'sta'
        #option encryption       'psk'
        #option key      'somekey'

        # Station mode - WEP
        #option mode     sta
        #option ssid     some-ssid
        #option encryption wep
        #option key      1
        #option key1     '36241D3FFEF916BE7DCF995FD9'

        # AP mode
        #option mode     ap
        #option encryption none
        #option ssid     OpenWrt


